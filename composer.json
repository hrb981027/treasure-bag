{
    "name": "hrb981027/treasure-bag",
    "type": "library",
    "license": "proprietary",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "autoload": {
        "files": [
            "src/Functions.php"
        ],
        "psr-4": {
            "Hrb981027\\TreasureBag\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.3",
        "hyperf/constants": "^2.2",
        "hyperf/guzzle": "^2.2",
        "hyperf/utils": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "hyperf/database": "^2.2",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "^4.5",
        "swow/swow": "dev-develop",
        "symfony/var-dumper": "^5.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always"
    },
    "extra": {
        "hyperf": {
            "config": "Hrb981027\\TreasureBag\\ConfigProvider"
        }
    }
}
